{
  "env": {
    "node": true,
    "es6": true
  },
  "globals": {
    "Game": "readonly"
  },
  "parser": "babel-eslint",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": [
      "error",
      "never"
    ],
    "eqeqeq": [
      "error",
      "always"
    ],
    "getter-return": [
      "error"
    ],
    "no-setter-return": [
      "error"
    ],
    "accessor-pairs": [
      "error"
    ],
    "class-methods-use-this": [
      "warn"
    ],
    "default-case": [
      "error"
    ],
    "no-constructor-return": [
      "error",
      "always"
    ],
    "no-invalid-this": [
      "error"
    ],
    "camelcase": [
      "error"
    ],
    "comma-spacing": [
      "error",
      {
        "after": true,
        "before": false
      }
    ],
    "comma-style": [
      "error",
      "last"
    ],
    "new-cap": [
      "error"
    ],
    "no-trailing-spaces": [
      "error"
    ],
    "arrow-body-style": [
      "error",
      "always"
    ],
    "arrow-parens": [
      "error"
    ],
    "arrow-spacing": [
      "error",
      {
        "before": true,
        "after": true
      }
    ],
    "no-const-assign": [
      "error"
    ],
    "no-var": [
      "error"
    ],
    "prefer-const": [
      "error"
    ]
  }
}